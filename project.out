SQL> CREATE TABLE Athlete
  2  (
  3  aid INTEGER,
  4  lname CHAR(30) NOT NULL,
  5  fname CHAR(30) NOT NULL,
  6  country CHAR(35) NOT NULL,
  7  mentorID INTEGER,
  8  --
  9  --
 10  CONSTRAINT AC1 PRIMARY KEY (aid),
 11  CONSTRAINT AC2 FOREIGN KEY (mentorID) REFERENCES Athlete(aid)
 12  	     ON DELETE CASCADE
 13  	     DEFERRABLE INITIALLY DEFERRED,
 14  CONSTRAINT AC3 FOREIGN KEY (country) REFERENCES Country(cname)
 15  	     ON DELETE CASCADE
 16  	     DEFERRABLE INITIALLY DEFERRED,
 17  CONSTRAINT AC4 CHECK(mentorID != aid)
 18  );

