SQL> CREATE TABLE Sponsors
  2  (
  3  eid INTEGER,
  4  sponsor_name CHAR(30) NOT NULL,
  5  --
  6  -- SC1: Event ID and sponsor name are the primary key of Sponsors
  7  CONSTRAINT SC1 PRIMARY KEY (eid, sponsor_name),
  8  -- SC2: Event ID of sponsors is a foreign key of te eid from Event
  9  -- On deletion it will cascade.
 10  CONSTRAINT SC2 FOREIGN KEY (eid) REFERENCES Event(eid)
 11  	 ON DELETE CASCADE
 12  	 DEFERRABLE INITIALLY DEFERRED
 13  );

